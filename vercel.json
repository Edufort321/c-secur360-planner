{
  "rewrites": [
    { "source": "/", "destination": "/c-secur360-planner-main/index.html" },
    { "source": "/(.*)", "destination": "/c-secur360-planner-main/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}